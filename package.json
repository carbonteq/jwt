{
  "name": "@carbonteq/jwt",
  "description": "JWT lib with native bindings and (optional) LRU cache",
  "version": "0.2.0",
  "main": "index.js",
  "types": "index.d.ts",
  "scope": "@carbonteq",
  "repository": {
    "type": "git",
    "url": "https://github.com/carbonteq/jwt"
  },
  "bugs": {
    "url": "https://github.com/carbonteq/hexapp/issues"
  },
  "homepage": "https://github.com/carbonteq/jwt#readme",
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "exports": {
    "node": {
      "require": "./index.js",
      "import": "./index.js",
      "types": "./index.d.ts"
    }
  },
  "napi": {
    "name": "jwt",
    "triples": {
      "defaults": false,
      "additional": [
        "x86_64-pc-windows-msvc",
        "x86_64-unknown-linux-gnu",
        "universal-apple-darwin"
      ]
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@napi-rs/cli": "^2.16.1",
    "@types/benchmark": "^2.1.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.2.6",
    "ava": "^5.1.1",
    "benchmark": "^2.1.4",
    "chalk": "^5.2.0",
    "esbuild": "^0.18.0",
    "esbuild-runner": "^2.2.2",
    "fast-jwt": "^3.1.1",
    "jose": "^4.14.4",
    "jsonwebtoken": "^9.0.0",
    "rome": "^12.1.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "ava": {
    "timeout": "1m",
    "failFast": false,
    "require": [
      "esbuild-runner/register"
    ],
    "extensions": [
      "cjs",
      "mjs",
      "js",
      "ts"
    ]
  },
  "engines": {
    "node": ">= 16"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "ava",
    "universal": "napi universal",
    "version": "changeset version && napi version",
    "release": "changeset publish --no-git-tag"
  },
  "packageManager": "yarn@3.6.0"
}
